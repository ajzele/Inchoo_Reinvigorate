<config>
    <modules>
        <Inchoo_Reinvigorate>
            <version>1.0.0.0</version>
        </Inchoo_Reinvigorate>
    </modules>
    <global>
        <blocks>
            <inchoo_reinvigorate>
                <class>Inchoo_Reinvigorate_Block</class>
            </inchoo_reinvigorate>
        </blocks>
        <helpers>
            <inchoo_reinvigorate>
                <class>Inchoo_Reinvigorate_Helper</class>
            </inchoo_reinvigorate>
        </helpers>
    </global>
    <frontend>       
        <layout>
            <updates>
                <inchoo_reinvigorate>
                    <file>inchoo_reinvigorate.xml</file>
                </inchoo_reinvigorate>
            </updates>
        </layout>
    </frontend>     
    <default>
        <inchoo_reinvigorate>
            <settings>
                <active>0</active>
                <api_key></api_key>
            </settings>
        </inchoo_reinvigorate>
    </default>
</config>